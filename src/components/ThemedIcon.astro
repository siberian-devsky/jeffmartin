---
interface Props {
  width?: string;
  height?: string;
  class?: string;
}

const { width = "24", height = "24", class: className = "" } = Astro.props;
---

<svg 
  xmlns="http://www.w3.org/2000/svg" 
  width={width} 
  height={height} 
  viewBox="0 0 24 24" 
  fill="none" 
  stroke="currentColor" 
  stroke-width="1" 
  stroke-linecap="round" 
  stroke-linejoin="round" 
  class={`text-gray-800 dark:text-[rgba(255,255,255,0.608)] ${className}`}
>
  <slot />
</svg>